\cf8 # Chi\'b2 pour chaque mod\'e8le (en supposant erreur 0.3 dex sur les masses)\cf9 \
err \cf11 =\cf9  \cf13 0.3\cf9 \
\
chi2_lcdm \cf11 =\cf9  np.\cf12 sum\cf9 (((obs_mass \cf11 -\cf9  lcdm_max) \cf11 /\cf9  err)\cf11 **\cf13 2\cf9 )\
chi2_janus \cf11 =\cf9  np.\cf12 sum\cf9 (((obs_mass \cf11 -\cf9  janus_max) \cf11 /\cf9  err)\cf11 **\cf13 2\cf9 )\
\
\cf8 # Degr\'e9s de libert\'e9\cf9 \
ndof \cf11 =\cf9  \cf12 len\cf9 (obs_mass) \cf11 -\cf9  \cf13 1\cf9   \cf8 # \uc0\u923 CDM: 0 param\'e8tres libres\cf9 \
ndof_janus \cf11 =\cf9  \cf12 len\cf9 (obs_mass) \cf11 -\cf9  \cf13 2\cf9   \cf8 # JANUS: 1 param\'e8tre (\uc0\u945 )\cf9 \
\
\cf8 # p-values\cf9 \
p_lcdm \cf11 =\cf9  \cf13 1\cf9  \cf11 -\cf9  chi2.cdf(chi2_lcdm, ndof)\
p_janus \cf11 =\cf9  \cf13 1\cf9  \cf11 -\cf9  chi2.cdf(chi2_janus, ndof_janus)\
