\f2 \uc0\u8594 
\f0 \'e9toiles)\cf9 \
    \cf8 # - Temps d'assemblage = 50% du temps cosmique\cf9 \
    \
    SFR_max \cf11 =\cf9  \cf13 80\cf9   \cf8 # M
\f1 \'a8\'91
\f0 /yr\cf9 \
    efficiency \cf11 =\cf9  \cf13 0.10\cf9 \
